{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: TorchRun",
            "type": "debugpy",
            "request": "launch",
            "program": "-m",
            "cwd":"/teamspace/studios/this_studio/sfd-main",
            "args": [
                "torch.distributed.run",
                "--standalone",
                "--nproc_per_node=1",
                "--master_port=12345",
                "train.py",
                "--dataset_name=cifar10",
                "--total_kimg=200",
                "--batch=256",
                "--lr=5e-5",
                "--num_steps=4",
                "--M=3",
                "--afs=False",
                "--sampler_tea=dpmpp",
                "--max_order=3",
                "--predict_x0=True",
                "--lower_order_final=True",
                "--schedule_type=polynomial",
                "--schedule_rho=7",
                "--use_step_condition=False",
                "--is_second_stage=False"
            ],
            "console": "integratedTerminal",
            "justMyCode": false

        }
    ]
}
